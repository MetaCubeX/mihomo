{
    "inbounds": [
        {
            "port": 10002,
            "listen": "0.0.0.0",
            "protocol": "trojan",
            "settings": {
                "clients": [
                    {
                        "password": "example",
                        "email": "grpc@example.com"
                    }
                ]
            },
            "streamSettings": {
                "network": "grpc",
